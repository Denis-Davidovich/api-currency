{
    "name": "vendor_name/api-currency",
    "description": "API with Symfony",
    "type": "project",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "proprietary",
    "authors": [
        {
            "name": "denis.davidovich",
            "email": "email@example.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/asset": "^7.2",
        "symfony/dotenv": "^7.2",
        "symfony/form": "^8.0",
        "symfony/framework-bundle": "^7.2",
        "symfony/runtime": "^7.2",
        "symfony/twig-bundle": "^7.2",
        "symfony/validator": "^8.0",
        "symfony/yaml": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "symfony/debug-bundle": "^7.2",
        "symfony/maker-bundle": "^1.62",
        "symfony/test-pack": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "APP_ENV=test phpunit"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    }
}
